    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "yarn run check:ts && cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./bin/app.ts",
        "dev:server": "yarn run check:ts && cross-env NODE_ENV=development nodemon",
        "prod": "yarn run check:ts && cross-env NODE_ENV=production EDITABLE=false nodemon",
        "start": "cross-env NODE_ENV=production EDITABLE=false node ./bin/www",
        "build:dev": "yarn run check:ts && yarn run clean:dev && cross-env NODE_ENV=development webpack --mode development --progress --display-error-details",
        "build:prod": "yarn run check:ts && yarn run clean:prod && cross-env EDITABLE=false NODE_ENV=production webpack --mode production --progress --display-error-details",
        "test": "jest --config=./test/jest.config.json",
        "test:watch": "yarn run test --watchAll",
        "clean:module": "rimraf -rf node_modules",
        "clean:dev": "rimraf -rf public",
        "clean:prod": "rimraf -rf dist",
        "clean:bundle": "yarn run clean:dev && yarn run clean:prod",
        "init": "yarn run clean:module && yarn install",
        "check:ts": "tsc --noEmit",
        "check:watch": "yarn run check:ts -- --watch"
    },
